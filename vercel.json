{
  "version": 2,
  "builds": [
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/frontend/index.html"
    },
    {
      "src": "/login",
      "dest": "/frontend/login-fixed.html",
      "status": 200
    },
    {
      "src": "/login/",
      "dest": "/frontend/login-fixed.html",
      "status": 200
    },
    {
      "src": "/register",
      "dest": "/frontend/election-registration.html"
    },
    {
      "src": "/register/",
      "dest": "/frontend/election-registration.html"
    },
    {
      "src": "/admin-dashboard",
      "dest": "/frontend/admin-dashboard-enhanced.html"
    },
    {
      "src": "/admin-dashboard/",
      "dest": "/frontend/admin-dashboard-enhanced.html"
    },
    {
      "src": "/portal",
      "dest": "/frontend/personal-portal.html"
    },
    {
      "src": "/portal/",
      "dest": "/frontend/personal-portal.html"
    },
    {
      "src": "/verify",
      "dest": "/frontend/student-verification.html"
    },
    {
      "src": "/verify/",
      "dest": "/frontend/student-verification.html"
    },
    {
      "src": "/signup-complete",
      "dest": "/frontend/signup-complete.html"
    },
    {
      "src": "/signup-complete/",
      "dest": "/frontend/signup-complete.html"
    },
    {
      "src": "/home",
      "dest": "/frontend/index.html"
    },
    {
      "src": "/home/",
      "dest": "/frontend/index.html"
    },
    {
      "src": "/index",
      "dest": "/frontend/index.html"
    },
    {
      "src": "/index/",
      "dest": "/frontend/index.html"
    },
    {
      "src": "/config.js",
      "dest": "/frontend/config.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
