{
  "version": 2,
  "buildCommand": "cd saku-strategy/backend && pip install -r requirements.txt && python manage.py collectstatic --noinput",
  "outputDirectory": "saku-strategy/frontend",
  "installCommand": "cd saku-strategy/backend && pip install -r requirements.txt",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*\\.html)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

